{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "createrFsID": {
      "type": "string"
    },
    "category": {
      "type": "string",
      "required": true
    },
    "company": {
      "type": "string"
    },
    "taxID": {
      "type": "string"
    },
    "documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::document.document",
      "mappedBy": "client"
    },
    "fsID": {
      "type": "string"
    },
    "deleted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "client"
    },
    "promos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::promo.promo",
      "mappedBy": "clients"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user-info.user-info",
      "inversedBy": "clients"
    },
    "establishment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::establishment.establishment",
      "inversedBy": "clients"
    },
    "login": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "client_info"
    },
    "preferred_language": {
      "type": "string",
      "default": "fr"
    }
  }
}
